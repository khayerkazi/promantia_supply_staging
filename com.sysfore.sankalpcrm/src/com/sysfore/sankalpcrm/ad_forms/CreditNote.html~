<script type="text/javascript">
$(function() {
  var creditnoteid = $('#paramCreditNoteId').val();
  if(creditnoteid == ""){
  	$('#searchfilters').hide();
  }else{
  	$('#searchfilters').show();
  	$('#headerfilters').hide();	  
  }

});

function checkMember(){
  var memberid = $('#paramProductId').val();
  if(memberid==""){
   alert('You need to choose a member');
  }else{
   submitCommandForm('GENERATE', true, null, null, '_self', false);
  }
}
function printClear(){
	setTimeout(printClear, 5000);
	submitCommandForm('DISCARD', true, null, null, '_self', false);
}
function doProcess(){
	openServletNewWindow('PRINT', false, 'CreditNote.html', 'CreditNote', null, true, 700, 900, null, null, null, null, true);
	setTimeout(doComplete, 7000);
}
function doComplete()
{
	submitCommandForm('COMPLETE', true, null, null, '_self', false);
}
</script>
