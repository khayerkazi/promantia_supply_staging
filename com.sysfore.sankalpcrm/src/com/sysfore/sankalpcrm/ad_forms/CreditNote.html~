    <script language="JavaScript" type="text/javascript" id="paramScript">
        function validate(action) {
        alert(action+" funcEvenOddRow ");
        var frm=document.frmMain;
        if (action=="GENERATE") {
        if (radioValue(frm.inpswSrequisitionId)==null) {
        showJSMessage(1);
        return false;
        } else if (frm.inpswSrequisitionId.checked) {
        if (document.getElementsByName("inppricenew"+frm.inpswSrequisitionId.value)[0].value==null ||
        document.getElementsByName("inppricenew"+frm.inpswSrequisitionId.value)[0].value=="") {
        setWindowElementFocus(document.getElementsByName("inppricenew_D"+frm.inpswSrequisitionId.value)[0]);
        showJSMessage(1);
        return false;
        }
        if (document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId.value)[0].value==null ||
        document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId.value)[0].value=="") {
        setWindowElementFocus(document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId.value)[0]);
        showJSMessage(1);
        return false;
        }
        } else {
        var total = frm.inpswSrequisitionId.length;
        for (var i=0; i; i++){
        if (frm.inpswSrequisitionId[i].checked) {
        if (document.getElementsByName("inppricenew"+frm.inpswSrequisitionId[i].value)[0].value==null ||
        document.getElementsByName("inppricenew"+frm.inpswSrequisitionId[i].value)[0].value=="") {
        setWindowElementFocus(document.getElementsByName("inppricenew_D"+frm.inpswSrequisitionId[i].value)[0]);
        showJSMessage(1);
        return false;
        }
        if (document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId[i].value)[0].value==null ||
        document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId[i].value)[0].value=="") {
        setWindowElementFocus(document.getElementsByName("inpqtyneword"+frm.inpswSrequisitionId[i].value)[0]);
        showJSMessage(1);
        return false;
        }
        }
        }
        }
        } else if (action=="FIND") {
        if (frm.inpadOrgId.value==null || frm.inpadOrgId.value=="") {
        setWindowElementFocus(frm.inpadOrgId);
        showJSMessage(7);
        return false;
        }
        else if (action=="DELETE") {
        if (radioValue(frm.inpswCreditNoteLineId)==null) {
        showJSMessage(1);
        return false;
        }
        else if (frm.inpswCreditNoteLineId.checked) {

        }
        }
        return true;
        }

    </script>

    <SCRIPT language="JavaScript" type="text/javascript">
        function onLoadDo(){
        this.windowTables = new Array(
        new windowTableId('client', 'buttonHTML')
        );
        setWindowTableParentElement();
        this.tabsTables = new Array(
        new tabTableId('tdtopTabs')
        );
        setTabTableParentElement();
        enableShortcuts('edition');
        setBrowserAutoComplete(false);
        resizeArea();
        updateMenuIcon('buttonMenu');

        setWindowElementFocus('firstElement');
        }

        function onResizeDo(){
        resizeArea();
        }
    </SCRIPT>
