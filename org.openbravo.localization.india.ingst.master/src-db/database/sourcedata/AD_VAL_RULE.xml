<!--5DC209B0D2024BAC9C7F78260C9EFD89-->  <NAME><![CDATA[GSTIN Org Validation]]></NAME>
<!--5DC209B0D2024BAC9C7F78260C9EFD89-->  <DESCRIPTION><![CDATA[Validation for getting legal org GSTIN or Parent org GSTIN no]]></DESCRIPTION>
<!--5DC209B0D2024BAC9C7F78260C9EFD89-->  <TYPE><![CDATA[S]]></TYPE>
<!--5DC209B0D2024BAC9C7F78260C9EFD89-->  <CODE><![CDATA[ad_org_id =ad_get_org_le_bu(@AD_ORG_ID@,'LE')  and c_bpartner_id is null 
AND substring(uidno, 1, 2) in(select c_region.em_ingst_statecode 
from ad_orginfo  
join c_location  on ad_orginfo.c_location_id=c_location.c_location_id 
join c_region  on c_location.c_region_id=c_region.c_region_id 
where  ad_orginfo.ad_org_id =@AD_ORG_ID@
 and c_region.c_country_id =(select c_country_id from c_country where name='India'))]]></CODE>
