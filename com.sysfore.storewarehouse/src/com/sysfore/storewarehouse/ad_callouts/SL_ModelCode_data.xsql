<?xml version="1.0" encoding="UTF-8" ?>


<SqlClass name="SLModelCodeData" package="com.sysfore.storewarehouse.ad_callouts">
  <SqlMethod name="select" type="preparedStatement" return="multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
      <![CDATA[
     Select name, cl_department_id,  cl_brand_id, cl_hscode
     from cl_model
     where value=?
      ]]>
    </Sql>
    <Parameter name="modelcode"/>
  </SqlMethod>

</SqlClass>