+    where.append("                          and sd.quantityInDraftTransactions = 0");
+    where
+        .append("                          and coalesce(sd.quantityOrderInDraftTransactions, 0) = 0 ");
+    where.append("              )");
+    where.append("          or :qtyRange = '=' ");
+    where.append("        )");
+    where.append("    and p.isGeneric = 'N'");
+    where.append("    and (:excludeReserved = 'N' or sd.reservedQty = 0)");
+    where.append(" order by sd.storageBin, p.searchKey, sd.creationDate");
+
+    OBQuery<StorageDetail> sdQry = OBDal.getInstance().createQuery(StorageDetail.class,
+        where.toString());
+    sdQry.setFilterOnReadableOrganization(false);
+    sdQry.setNamedParameter("clientId", strClientId);
+    if (strProductIdList.size() > 0) {
+      sdQry.setNamedParameter("productIdList", strProductIdList);
+    }
+    if (strStorageBinIdList.size() > 0) {
+      sdQry.setNamedParameter("storageBinIdList", strStorageBinIdList);
+    }
+    if (abc != null) {
+      sdQry.setNamedParameter("abc", abc);
+    }
+    if (strWarehouseId != null) {
+      sdQry.setNamedParameter("warehouseId", strWarehouseId);
+    }
+    if (strProductCategoryId != null) {
+      sdQry.setNamedParameter("productCategoryId", strProductCategoryId);
+    }
+    sdQry.setNamedParameter("inventoryId", strInventoryId);
+    sdQry.setNamedParameter("qtyRange", qtyRange);
+    sdQry.setNamedParameter("excludeReserved", excludeReserved ? "Y" : "N");
+
+    return sdQry.scroll(ScrollMode.FORWARD_ONLY);
+  }
+}
