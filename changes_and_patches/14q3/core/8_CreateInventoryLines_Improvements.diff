+  /**
+   * Get an Inventory Line, if exists, that fulfills the conditions of the parameters
+   */
+  private List<InventoryCountLine> getInventoryLine(InventoryCount inventory, Product product,
+      Locator locator, AttributeSetInstance attribute, UOM uom, ProductUOM orderUom) {
+    OBCriteria<InventoryCountLine> obc = OBDal.getInstance().createCriteria(
+        InventoryCountLine.class);
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_PHYSINVENTORY, inventory));
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_PRODUCT, product));
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_STORAGEBIN, locator));
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_ATTRIBUTESETVALUE, attribute));
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_UOM, uom));
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_ORDERUOM, orderUom));
+
+    return obc.list();
+  }
