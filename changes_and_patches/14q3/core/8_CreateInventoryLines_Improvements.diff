+  /**
+   * Return a ScrollableResult that contains all the Products that are not present in the selected
+   * Storage Bin
+   */
+  private ScrollableResults getProductsWithZeroStock(String strClientId, String locatorId) {
+    StringBuffer where = new StringBuffer();
+    where.append(" as p");
+    where.append(" where p.client.id = :clientId");
+    where.append("   and p.isGeneric = 'N'");
+    where.append("   and p.stocked = 'Y'");
+    where.append("   and not exists (select sd");
+    where.append("                   from MaterialMgmtStorageDetail sd");
+    where.append("                   where sd.product = p");
+    where.append("                   and sd.storageBin in :storageBinId");
+
+    OBQuery<Product> productQry = OBDal.getInstance().createQuery(Product.class, where.toString());
+    productQry.setFilterOnReadableOrganization(false);
+    productQry.setNamedParameter("clientId", strClientId);
+    productQry.setNamedParameter("storageBinId", locatorId);
+
+    return productQry.scroll(ScrollMode.FORWARD_ONLY);
+  }
