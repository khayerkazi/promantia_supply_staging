+      message.setType("Error");
+      message.setTitle(OBMessageUtils.messageBD("Error"));
+      message.setMessage(OBMessageUtils.messageBD("@AlreadyPosted@"));
+      return message;
+    }
+
+    // Create 0 Storage Records
+    try {
+      int createdStorageRecords = 0;
+      if ("=".equals(qtyRange)) {
