+  /**
+   * Get last Line Number from Inventory Lines
+   */
+  private Long getLastLineNoFromInventoryLines(InventoryCount inventory) {
+    OBCriteria<InventoryCountLine> obc = OBDal.getInstance().createCriteria(
+        InventoryCountLine.class);
+    obc.add(Restrictions.eq(InventoryCountLine.PROPERTY_PHYSINVENTORY, inventory));
+    ProjectionList projections = Projections.projectionList();
+    projections.add(Projections.max(InventoryCountLine.PROPERTY_LINENO));
+    obc.setProjection(projections);
+    Long lineNo = (Long) obc.uniqueResult();
+    ;
+    if (lineNo == null) {
+      lineNo = (long) 0;
+    }
+    return lineNo;
+  }
