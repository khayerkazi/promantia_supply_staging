+  /**
+   * Insert Inventory Line
+   * 
+   * @return
+   */
+  private InventoryCountLine insertInventoryLine(StorageDetail storageDetail,
+      InventoryCount inventory, Long lineNo, boolean regularization) {
+    InventoryCountLine inventoryLine = OBProvider.getInstance().get(InventoryCountLine.class);
+    inventoryLine.setActive(true);
+    inventoryLine.setCreatedBy(inventory.getCreatedBy());
+    inventoryLine.setCreationDate(new Date());
+    inventoryLine.setUpdated(new Date());
+    inventoryLine.setUpdatedBy(inventory.getCreatedBy());
+    inventoryLine.setClient(storageDetail.getClient());
+    inventoryLine.setOrganization(storageDetail.getOrganization());
+    inventoryLine.setPhysInventory(inventory);
+    inventoryLine.setProduct(storageDetail.getProduct());
+    inventoryLine.setStorageBin(storageDetail.getStorageBin());
+    inventoryLine.setAttributeSetValue(storageDetail.getAttributeSetValue());
+    inventoryLine.setBookQuantity(storageDetail.getQuantityOnHand());
+    inventoryLine.setQuantityCount(regularization ? BigDecimal.ZERO : (storageDetail
+        .getQuantityOnHand() != null && storageDetail.getQuantityOnHand()
+        .compareTo(BigDecimal.ZERO) == -1) ? BigDecimal.ZERO : storageDetail.getQuantityOnHand());
+    inventoryLine.setReservedQty(storageDetail.getReservedQty());
+    inventoryLine.setLineNo(lineNo);
+    inventoryLine.setUOM(storageDetail.getUOM());
+    if (storageDetail.getOrderUOM() != null) {
+      inventoryLine.setOrderUOM(storageDetail.getOrderUOM());
+      inventoryLine.setQuantityOrderBook(storageDetail.getOnHandOrderQuanity());
+      inventoryLine.setOrderQuantity(regularization ? BigDecimal.ZERO : (storageDetail
+          .getOnHandOrderQuanity() != null && storageDetail.getOnHandOrderQuanity().compareTo(
+          BigDecimal.ZERO) == -1) ? BigDecimal.ZERO : storageDetail.getOnHandOrderQuanity());
+    }
+
+    OBDal.getInstance().save(inventoryLine);
+    return inventoryLine;
+  }
+
+  /**
+   * Update existing Inventory Line
+   */
+  private void updateInventoryLine(InventoryCountLine inventoryLine, StorageDetail storageDetail,
+      User createdBy, boolean regularization) {
+    inventoryLine.setBookQuantity(storageDetail.getQuantityOnHand());
+    inventoryLine.setQuantityCount(regularization ? BigDecimal.ZERO : (storageDetail
+        .getQuantityOnHand() != null && storageDetail.getQuantityOnHand()
+        .compareTo(BigDecimal.ZERO) == -1) ? BigDecimal.ZERO : storageDetail.getQuantityOnHand());
+    inventoryLine.setReservedQty(storageDetail.getReservedQty());
+    inventoryLine.setUpdated(new Date());
+    inventoryLine.setUpdatedBy(createdBy);
+    if (storageDetail.getOrderUOM() != null) {
+      inventoryLine.setQuantityOrderBook(storageDetail.getQuantityOnHand());
+      inventoryLine.setOrderQuantity(regularization ? BigDecimal.ZERO : (storageDetail
+          .getOnHandOrderQuanity() != null && storageDetail.getOnHandOrderQuanity().compareTo(
+          BigDecimal.ZERO) == -1) ? BigDecimal.ZERO : storageDetail.getQuantityOnHand());
+    }
+    OBDal.getInstance().save(inventoryLine);
+  }
